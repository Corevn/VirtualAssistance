<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity Notification.
    -->
    <changeSet id="20190313134945-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="head_quater_id"
                                 baseTableName="notification"
                                 constraintName="fk_notification_head_quater_id"
                                 referencedColumnNames="id"
                                 referencedTableName="head_quater"/>

        <addForeignKeyConstraint baseColumnNames="notifications_id"
                                 baseTableName="notification_document_type"
                                 constraintName="fk_notification_document_type_notifications_id"
                                 referencedColumnNames="id"
                                 referencedTableName="notification"/>
        <addForeignKeyConstraint baseColumnNames="document_types_id"
                                 baseTableName="notification_document_type"
                                 constraintName="fk_notification_document_type_document_types_id"
                                 referencedColumnNames="id"
                                 referencedTableName="document_type"/>
        
    </changeSet>
</databaseChangeLog>
